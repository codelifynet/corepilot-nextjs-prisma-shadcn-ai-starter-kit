{
  "name": "corepilot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3007",
    "build": "npm run type-check:src && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit --pretty",
    "type-check:src": "tsc --noEmit --pretty --skipLibCheck --project tsconfig.src.json",
    "type-check:watch": "tsc --noEmit --watch --pretty",
    "prebuild": "npm run type-check:src",
    "postinstall": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "biome:lint": "biome lint ./src",
    "biome:lint:fix": "biome lint ./src --write",
    "biome:format": "biome format ./src",
    "biome:format:fix": "biome format ./src --write",
    "biome:check": "biome check ./src",
    "biome:check:fix": "biome check ./src --write"
  },
  "prisma": {
    "schema": "./prisma",
    "seed": "tsx prisma/seed.ts"
  },
